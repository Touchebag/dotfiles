[push]
  # Set default pushing behaviour
  default = simple
[core]
  # Set default commit editor to vim
  editor = vim
  # Set global gitignore file
  excludesfile = ~/.gitignore
[filter "gitignore"]
  # Do not commit lines ending in #gitignore (e.g. debug lines)
  clean = "sed '/#gitignore$/'d"
  smudge = cat
[color]
	ui = auto
[gitreview]
	remote = origin
